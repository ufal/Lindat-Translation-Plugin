<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
  <Id>f3eb0f33-9ecb-4f23-8b41-494236c4e732</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Contoso</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="LINDAT"/>
  <Description DefaultValue="A template to get started."/>
  <IconUrl DefaultValue="https://localhost:3000/assets/lindatIcon-32.png"/>
  <HighResolutionIconUrl DefaultValue="https://localhost:3000/assets/lindatIcon-64.png"/>
  <SupportUrl DefaultValue="https://www.contoso.com/help"/>
  <AppDomains>
    <AppDomain>https://www.contoso.com</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Document"/>
  </Hosts>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://localhost:3000/taskpane.html"/>
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Document">
        <DesktopFormFactor>
          <GetStarted>
            <Title resid="GetStarted.Title"/>
            <Description resid="GetStarted.Description"/>
            <LearnMoreUrl resid="GetStarted.LearnMoreUrl"/>
          </GetStarted>
          <FunctionFile resid="Commands.Url"/>
          <ExtensionPoint xsi:type="PrimaryCommandSurface">
            <OfficeTab id="TabHome">
              <Group id="CommandsGroup">
                <Label resid="CommandsGroup.Label"/>
                <Icon>
                  <bt:Image size="16" resid="LindatIcon-16"/>
                  <bt:Image size="32" resid="LindatIcon-32"/>
                  <bt:Image size="80" resid="LindatIcon-80"/>
                </Icon>


                <Control xsi:type="Button" id="UDPipeButton">
                  <Label resid="UDPipeButton.Label"/>
                  <Supertip>
                    <Title resid="UDPipeButton.Label"/>
                    <Description resid="UDPipeButton.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="UDPipeIcon-16"/>
                    <bt:Image size="32" resid="UDPipeIcon-32"/>
                    <bt:Image size="80" resid="UDPipeIcon-80"/>
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>UDPipeIcon</TaskpaneId>
                    <SourceLocation resid="UDPipe.Url"/>
                  </Action>
                </Control>


                <Control xsi:type="Button" id="NameTagButton">
                  <Label resid="NameTagButton.Label"/>
                  <Supertip>
                    <Title resid="NameTagButton.Label"/>
                    <Description resid="NameTagButton.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="NameTagIcon-16"/>
                    <bt:Image size="32" resid="NameTagIcon-32"/>
                    <bt:Image size="80" resid="NameTagIcon-80"/>
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>NameTag</TaskpaneId>
                    <SourceLocation resid="NameTag.Url"/>
                  </Action>
                </Control>


                <Control xsi:type="Button" id="TranslatorButton">
                  <Label resid="TranslatorButton.Label"/>
                  <Supertip>
                    <Title resid="TranslatorButton.Label"/>
                    <Description resid="TranslatorButton.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="TranslatorIcon-16"/>
                    <bt:Image size="32" resid="TranslatorIcon-32"/>
                    <bt:Image size="80" resid="TranslatorIcon-80"/>
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>Translator</TaskpaneId>
                    <SourceLocation resid="Translator.Url"/>
                  </Action>
                </Control>


              </Group>
            </OfficeTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <bt:Image id="LindatIcon-16" DefaultValue="https://localhost:3000/assets/lindatIcon-16.png"/>
        <bt:Image id="LindatIcon-32" DefaultValue="https://localhost:3000/assets/lindatIcon-32.png"/>
        <bt:Image id="LindatIcon-80" DefaultValue="https://localhost:3000/assets/lindatIcon-80.png"/>
        
        <bt:Image id="UDPipeIcon-16" DefaultValue="https://localhost:3000/assets/UDPipeIcon-16.png"/>
        <bt:Image id="UDPipeIcon-32" DefaultValue="https://localhost:3000/assets/UDPipeIcon-32.png"/>
        <bt:Image id="UDPipeIcon-80" DefaultValue="https://localhost:3000/assets/UDPipeIcon-80.png"/>
        
        <bt:Image id="NameTagIcon-16" DefaultValue="https://localhost:3000/assets/NameTagIcon-16.png"/>
        <bt:Image id="NameTagIcon-32" DefaultValue="https://localhost:3000/assets/NameTagIcon-32.png"/>
        <bt:Image id="NameTagIcon-80" DefaultValue="https://localhost:3000/assets/NameTagIcon-80.png"/>
        
        <bt:Image id="TranslatorIcon-16" DefaultValue="https://localhost:3000/assets/TranslatorIcon-16.png"/>
        <bt:Image id="TranslatorIcon-32" DefaultValue="https://localhost:3000/assets/TranslatorIcon-32.png"/>
        <bt:Image id="TranslatorIcon-80" DefaultValue="https://localhost:3000/assets/TranslatorIcon-80.png"/>
      </bt:Images>
      <bt:Urls>
        <bt:Url id="GetStarted.LearnMoreUrl" DefaultValue="https://go.microsoft.com/fwlink/?LinkId=276812"/>
        <bt:Url id="Commands.Url" DefaultValue="https://localhost:3000/commands.html"/>
        <bt:Url id="UDPipe.Url" DefaultValue="https://localhost:3000/udpipe.html"/>
        <bt:Url id="NameTag.Url" DefaultValue="https://localhost:3000/nametag.html"/>
        <bt:Url id="Translator.Url" DefaultValue="https://localhost:3000/translator.html"/>
      </bt:Urls>
      <bt:ShortStrings>
      <bt:String id="GetStarted.Title" DefaultValue="Get started with your sample add-in!"/>
        <bt:String id="CommandsGroup.Label" DefaultValue="Commands Group"/>
        <bt:String id="GetStarted.Title" DefaultValue="LINDAT plugin"/>
        <bt:String id="CommandsGroup.Label" DefaultValue="LINDAT"/>
        <bt:String id="UDPipeButton.Label" DefaultValue="UDPipe"/>
        <bt:String id="NameTagButton.Label" DefaultValue="NameTag"/>
        <bt:String id="TranslatorButton.Label" DefaultValue="Translator"/>
      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="GetStarted.Description" DefaultValue="Your sample add-in loaded succesfully. Go to the HOME tab and click the 'Show Taskpane' button to get started."/>
        <bt:String id="GetStarted.Description" DefaultValue="LINDAT plugin installed successfully"/>
        <bt:String id="UDPipeButton.Tooltip" DefaultValue="Click to open UDPipe"/>
        <bt:String id="NameTagButton.Tooltip" DefaultValue="Click to open NameTag"/>
        <bt:String id="TranslatorButton.Tooltip" DefaultValue="Click to open Translator"/>
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>